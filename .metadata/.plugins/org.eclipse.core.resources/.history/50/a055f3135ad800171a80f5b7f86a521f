		s1.age = 12;
		s1.ID = 858754;
		s1.getAttendance();
		int result = s1.getGrades();
		System.out.println(result);
		Math.sqrt(4);

		Teacher t1 = new Teacher();
		t1.name = "Paul";
		t1.age = 72;
		t1.ID = 754894;
		t1.getAttendance();

		Staff stf = new Staff();
		stf.name = "Paul";
